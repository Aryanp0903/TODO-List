<!DOCTYPE html>
<html>
    <head>
        <title>TODO List</title>
        <Link rel="stylesheet" href="css/styles.css"></Link>
    </head>
    <body>
        <div class="box">
            <h1>Todo list</h1>
        </div>
        <div class="box">
            <%  for(i=0;i< ejes.length; i++){%>
                <div class="item">
                <input type="checkbox">
                <p><%= ejes[i] %></p>
            </div>
            <%} %>

            <form action="/" method="POST">
                <input type="text"  name="ele1" placeholder="Enter the task">
                <button type="submit">+</button>
            </form>

        </div>
    </body>
</html>